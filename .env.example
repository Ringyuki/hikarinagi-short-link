# 格式: postgresql://USER:PASSWORD@HOST:PORT/DB_NAME?schema=public
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/hikarinagi_short_link?schema=public"

JWT_SECRET="please-change-me-to-a-strong-secret"

NODE_ENV="development"

# 实际客户端 IP 的来源头
# 默认使用 Cloudflare 的 CF-Connecting-IP
IP_HEADER="cf-connecting-ip"

# 备用 IP 头（从左到右依次兜底，逗号分隔）
IP_FALLBACK_HEADERS="x-forwarded-for,x-real-ip,x-client-ip,fastly-client-ip"